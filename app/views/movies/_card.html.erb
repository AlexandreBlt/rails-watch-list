<div class="col-3">
<div class="card mb-3">
  <img src="<%= movie.poster_url%>" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= movie.title %></h5>
    <p class="card-text"><%= movie.overview %></p>
    <p class="card-text"><small class="text-body-secondary"><%= movie.rating %>/10 <i class="fa-solid fa-star"></i> </small></p>
  </div>
  <button class="btn btn-danger"  >add to list</button>
</div>
</div>
